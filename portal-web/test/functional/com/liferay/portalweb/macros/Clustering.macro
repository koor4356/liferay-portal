definition {
	macro viewClusterStatusInConsole {
		AssertConsoleTextPresent(value1 = "Accepted view");

		AssertConsoleTextPresent(value1 = "com.liferay.portal.cluster.multiple");

		AssertConsoleTextPresent(value1 = "com.liferay.portal.cache.multiple");

		AssertConsoleTextPresent(value1 = "com.liferay.portal.scheduler.multiple");
	}

	macro viewLiveSessions {
		var key_navItem = "${navItem}";
		var key_index = "${number}";

		if (IsElementPresent(locator1 = "NavBar#ACTIVE_NAV_ITEM_LINK")) {
			AssertElementPresent(locator1 = "LexiconTable#TABLE_SESSION_ID");
		}

		var viewSessionId = selenium.getText("LexiconTable#TABLE_SESSION_ID");

		AssertTextPresent(locator1 = "LexiconTable#TABLE_SESSION_ID", value1 = "${viewSessionId}");
	}

	macro viewNoLiveSessions {
		var key_index = "${number}";

		AssertElementNotPresent(locator1 = "LexiconTable#TABLE_SESSION_ID");
	}
}