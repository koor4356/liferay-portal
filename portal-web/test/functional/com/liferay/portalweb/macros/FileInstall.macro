definition {

	macro deleteWarOffline {
		AntCommand(
			locator1 = "build-test.xml",
			value1 = "delete-dependency-wars  -Dliferay.dependency.wars=${liferayDependencyWars}");

		Pause(locator1 = "10000");
	}

	macro deployJarAndWarOnServer {
		AntCommand(
			locator1 = "build-test.xml",
			value1 = "deploy-dependency-jars -Dliferay.dependency.jars=${liferayDependencyJars}");

		Pause(locator1 = "10000");
	}

}